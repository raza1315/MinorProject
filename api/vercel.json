{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        { "handle": "filesystem" },
        {
            "src": "/socket.io/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "server": {
        "src": "./index.js",
        "command": "node ./index.js"
    }
}
